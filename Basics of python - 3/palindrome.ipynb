   "source": [
    "def is_palindrome():\n",
    "    text = input(\"Enter string:\")\n",
    "\n",
    "    l = len(text)\n",
    "    for i in range(l//2):\n",
    "        if text[i] != text[l-1-i]:\n",
    "            return False\n",
    "    return True"
   ]
  },
